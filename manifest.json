{
  "manifest_version": 2,
  "name": "Google Chrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
    "matches": [
      "<all_urls>"
      ],
    "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": { "16": "yoda-16.png",
      "32": "yoda-32.png",
      "64": "yoda-64.png",
      "128": "yoda-128.png",
      "256": "yoda-256.png"
    }
  },
  "permissions":["storage", "alarms", "notifications"]
}
